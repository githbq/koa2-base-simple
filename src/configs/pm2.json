{
    "apps": [
        {
            "name": "nodejs-tool-service",
            "script": "./src",
            "interpreter": "node",
            "error_file": "temp/logs/err.log",
            "out_file": "temp/logs/out.log",
            "merge_logs": true,
            "log_date_format": "YYYY-MM-DD HH:mm Z",
            "env": {
                "NODE_ENV": "production"
            },
            "node_args": [],
            "watch": false,
            "max_restarts": 100,
            "exec_mode": "cluster",
            "__pid_file": "./temp/app.pid"
        }
    ]
}